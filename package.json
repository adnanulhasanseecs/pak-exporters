{
  "name": "pak-exporters",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:port": "next dev -p",
    "validate:encoding": "tsx scripts/validate-encoding.ts",
    "detect-errors": "tsx scripts/detect-all-errors.ts",
    "setup:env": "tsx scripts/setup-env.ts",
    "type-check": "tsc --noEmit",
    "fix-errors": "tsx scripts/fix-common-errors.ts",
    "quick-check": "npm run type-check && npm run validate:encoding",
    "build": "npm run validate:encoding && next build",
    "start": "next start",
    "start:port": "next start -p",
    "lint": "eslint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage:run": "vitest --coverage --run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "test:visual:update": "playwright test --config=playwright-visual.config.ts --update-snapshots",
    "test:visual:ui": "playwright test --config=playwright-visual.config.ts --ui",
    "test:coverage:analyze": "tsx scripts/test-coverage.ts",
    "server:start": "powershell -ExecutionPolicy Bypass -File ./scripts/manage-pak-exporters.ps1 start",
    "server:stop": "powershell -ExecutionPolicy Bypass -File ./scripts/manage-pak-exporters.ps1 stop",
    "server:restart": "powershell -ExecutionPolicy Bypass -File ./scripts/manage-pak-exporters.ps1 restart",
    "server:status": "powershell -ExecutionPolicy Bypass -File ./scripts/manage-pak-exporters.ps1 status",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "tsx scripts/migrate-data.ts",
    "db:verify": "tsx scripts/verify-relationships.ts",
    "db:studio": "prisma studio",
    "db:backup": "tsx scripts/backup-database.ts",
    "db:restore": "tsx scripts/restore-database.ts",
    "db:migrate:prod": "NODE_ENV=production tsx scripts/migrate-production.ts",
    "db:rollback": "tsx scripts/rollback-migration.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.62.11",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.25",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.555.0",
    "next": "16.0.6",
    "next-intl": "^4.5.8",
    "next-seo": "^7.0.1",
    "next-themes": "^0.4.6",
    "prisma": "^6.19.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.54.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/glob": "^8.1.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "glob": "^13.0.0",
    "jsdom": "^27.2.0",
    "playwright": "^1.57.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
